			"settings": {
				"foreground": "#9EFFFF"
			}
		},
		{
			"name": "[CSS] - Color for Support",
			"scope": ["source.css support", "source.stylus support"],
			"settings": {
				"foreground": "#a5ff90"
			}
		},
		{
			"name": "[CSS] - Color for Constant",
