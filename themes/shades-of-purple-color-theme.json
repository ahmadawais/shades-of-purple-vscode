			}
		},
		{
			"name": "[HTML] - Color for Entity Name",
			"scope": "text.html.basic entity.name",
			"settings": {
				"foreground": "#9effff"
			}
		},
		{
			"name": "[HTML] - Color for ID value",
			"scope": "meta.toc-list.id.html",
			"settings": {
				"foreground": "#A5FF90"
			}
		},
		{
			"name": "[HTML] - Color for Entity Other",
			"scope": "text.html.basic entity.other",
			"settings": {
				"fontStyle": "italic",
